{
  "routes": [
    {
      "name": "Обработка запроса от VoxImplant",
      "match": [
        {
          "type": "path",
          "path": "/voximplant"
        },
        {
          "type": "method",
          "methods": [
            "post"
          ]
        }
      ],
      "actions": [
        {
          "type": "process",
          "processor": "csv",
          "name": "csv_data",
          "input": {
            "type": "jsonpointer",
            "pointer": "/request/body"
          },
          "options": {
            "header": true,
            "delimiter": ";"
          }
        },
        {
          "type": "combine",
          "name": "result",
          "input": {
            "type": "jsonpointer",
            "pointer": "/data/csv_data/0"
          },
          "output": {
            "type": "dict",
            "values": {
              "vacancy": {
                "type": "jsonpointer",
                "pointer": "/vacancy"
              },
              "name": {
                "type": "jsonpointer",
                "pointer": "/name"
              },
              "city": {
                "type": "jsonpointer",
                "pointer": "/city"
              },
              "surname": {
                "type": "value",
                "value": ""
              },
              "phone": {
                "type": "jsonpointer",
                "pointer": "/phone"
              },
              "status": {
                "type": "value",
                "value": "Обработан роботом"
              },
              "call_status": {
                "type": "value",
                "value": ""
              },
              "url": {
                "type": "value",
                "value": ""
              },
              "citizenship": {
                "type": "value",
                "value": "Россия"
              },
              "huntflow_applicant_id": {
                "type": "jsonpointer",
                "pointer": "/huntflow_applicant_id"
              },
              "huntflow_vacancy_id": {
                "type": "jsonpointer",
                "pointer": "/huntflow_vacancy_id"
              },
              "endpoint": {
                "type": "jsonpointer",
                "pointer": "/endpoint"
              },
              "attempt": {
                "type": "value",
                "value": 1
              }
            }
          }
        },
        {
          "type": "response",
          "code": 200,
          "output": {
            "type": "jsonpointer",
            "pointer": "/data/result"
          }
        },
        {
          "type": "sleep",
          "seconds": 0.2
        },
        {
          "type": "request",
          "name": "status",
          "request": {
            "full_url": "http://127.0.0.1:7777/vox/response",
            "method": "post",
            "json": {
              "type": "jsonpointer",
              "pointer": "/data/result"
            }
          }
        }
      ]
    }
  ]
}
